<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    
    <!-- Email server name DO NOT CHANGE -->
    <add key="ipc.mailserver" value="imap-mail.outlook.com"/>
    
    <!-- This is your email account user id-->
    <add key="ipc.username" value="ipcinvestments@outlook.com"/>
    
    <!-- This is your email account password -->
    <add key="ipc.password" value=""/>
    
    <!-- This is the from email address to read from  (service@autoniq.com)-->
    <add key="ipc.from" value="service@autoniq.com"/>

    <!-- This is the date from which the email scanning will look start looking forward from-->
    <add key="ipc.startdate" value="2016-03-01"/>

  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>